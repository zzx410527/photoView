<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zzx.photo"
    android:installLocation="preferExternal"
    android:versionCode="123"
    android:versionName="1.0">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name">
        <activity android:name="com.zzx.photo.LauncherActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.zzx.photo.SimpleSampleActivity" />
        <activity android:name="com.zzx.photo.ViewPagerActivity" />
        <activity android:name="com.zzx.photo.RotationSampleActivity" />
        <activity android:name="com.zzx.photo.ImageGalleryActivity" />
        <activity android:name="com.zzx.photo.BucketHomeFragmentActivity" />
    </application>

</manifest>
